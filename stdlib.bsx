# ANSI Escape Sequences
(func (ttyHome) (print "\x1b[H"))
(func (ttyClear) (print "\x1b[2J"))
(func (ttyGreen) (print "\x1b[32m"))
(func (ttyWhite) (print "\x1b[37m"))
(func (ttyXY X Y) (print ($ "x1b[" X ";" Y "H")))
